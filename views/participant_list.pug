extends layout

block content
  h1= title
  
  ul
    each participant in participant_list
      li 
        a(href=participant.url) #{participant.name} 
        //| <br>#{participant.address}

    else
      li There are no participants.


       div('text-align:center')
      div('display:inline-block')
    img.img-responsive(src='/images/TSTWomen.png', alt='Twist Participants')