extends layout

block content
  h1= title
  
  ul
    each session in session_list
      li 
        a(href=session.url) #{session.time} 
        //|  (#{book.author.name})

    else
      li There are no sessions.

      div('text-align:center')
      div('display:inline-block')
    img.img-responsive(src='/images/SessionsTWIST.png', alt='Twist Sessions')